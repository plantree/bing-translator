    "clear": "rm -rf ./dist && rm -rf ./test-dist"
  },
  "dependencies": {
    "@types/debug": "^4.1.8",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^17.0.41",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.9.10"
  }
}
