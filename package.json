{
  "name": "bing-translator",
  "version": "0.1.0",
  "description": "An easy way to get the translator service from Bing",
  "author": "plantree",
  "license": "MIT",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "keywords": [
    "bing",
    "translate",
    "translator",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/plantree/bing-translator"
  },
  "scripts": {
